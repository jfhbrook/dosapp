[dosbox]
language=
machine=svga_s3
captures=capture
memsize=16

[cpu]
core=auto
cputype=auto
cycles=auto
cycleup=10
cycledown=20

[mixer]
nosound=false
rate=44100
blocksize=1024
prebuffer=25

[sblaster]
sbtype=sbpro
sbbase=220
irq=7
dma=1
hdma=5
sbmixer=true
oplmode=auto
oplemu=default
oplrate=44100

[speaker]
pcspeaker=true
pcrate=44100

[autoexec]
MOUNT A {{ .Env.DOSAPP_DISK_A }}
MOUNT C {{ .Env.DOSAPP_DISK_C }}
MOUNT D {{ .Env.DOSAPP_STATE_HOME }}/apps/wordperfect/cdrom
